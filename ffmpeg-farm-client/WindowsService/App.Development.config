<?xml version="1.0" encoding="utf-8"?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <applicationSettings>
    <FFmpegFarm.WindowsService.Properties.Settings>
      <setting name="ControllerApi" serializeAs="String" xdt:Locator="Match(name)" xdt:Transform="Replace">
        <value>http://ffmpegctrl01udv.net.dr.dk:9000</value>
      </setting>
      <setting name="ForceTempFolder" serializeAs="String" xdt:Locator="Match(name)" xdt:Transform="Replace">
        <value>D:\Temp</value>
      </setting>
    </FFmpegFarm.WindowsService.Properties.Settings>
  </applicationSettings>
</configuration>